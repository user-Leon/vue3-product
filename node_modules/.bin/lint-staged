#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/A前端/codeweb/vue3-product/node_modules/.pnpm/lint-staged@13.3.0/node_modules/lint-staged/bin/node_modules:/mnt/e/A前端/codeweb/vue3-product/node_modules/.pnpm/lint-staged@13.3.0/node_modules/lint-staged/node_modules:/mnt/e/A前端/codeweb/vue3-product/node_modules/.pnpm/lint-staged@13.3.0/node_modules:/mnt/e/A前端/codeweb/vue3-product/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/A前端/codeweb/vue3-product/node_modules/.pnpm/lint-staged@13.3.0/node_modules/lint-staged/bin/node_modules:/mnt/e/A前端/codeweb/vue3-product/node_modules/.pnpm/lint-staged@13.3.0/node_modules/lint-staged/node_modules:/mnt/e/A前端/codeweb/vue3-product/node_modules/.pnpm/lint-staged@13.3.0/node_modules:/mnt/e/A前端/codeweb/vue3-product/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
else
  exec node  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
fi
